<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/ScrollView01" 
    android:layout_width="fill_parent" 
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/webbg"
    android:orientation="vertical"
   >
   
    <RelativeLayout 
        android:id="@+id/gTopliner" 
        android:layout_width="fill_parent" 
        android:layout_height="50dip"
        android:background="@drawable/topbg"
        >
    <Button
        android:layout_width="64dip"
        android:layout_height="30dip"
        android:id="@+id/btn_back" 
        android:layout_marginLeft="2dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/returnbutton1_style"
        android:text="@string/financehelper"
        android:textSize="10sp"
        />
    <TextView 
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="45dip"
        android:layout_marginTop="10dip"
        android:layout_toRightOf="@id/btn_back"
        android:layout_marginLeft="65dip"
        android:text="@string/userfunctuon"
        android:textSize="20sp"
    />
    
    </RelativeLayout>
    
    <LinearLayout 
       android:layout_marginTop="15dip" 
       android:layout_width="300dip"
       android:layout_height="wrap_content" 
       android:background="@drawable/one"
       android:orientation="vertical"
   >
   <LinearLayout 
       android:layout_width="300dip"
       android:layout_height="100dip" 
       android:orientation="vertical"
       >
   <TextView
       android:layout_marginTop="10dip" 
       android:layout_width="wrap_content"
       android:layout_height="40dip"
       android:text="(自2011-07-07生效)"
       android:textColor="#EE0000"
       android:layout_marginLeft="20dip"
       android:gravity="center"
       /> 
    <LinearLayout 
      android:layout_width="300dip"
      android:layout_height="50dip" 
      android:orientation="horizontal"
      android:layout_marginTop="5dip"  
     >
     <TextView 
       android:layout_width="140dip"
       android:layout_height="40dip"
       android:text="0-6月(含6月)"
    
       android:gravity="center"
       />     
      <TextView 
       android:layout_width="140dip"
       android:layout_height="40dip"
       android:text="6.10%"
      
       android:gravity="center"
       />   
    </LinearLayout> 
        
        
    </LinearLayout>
    <ImageView
       android:layout_marginLeft="10dip" 
       android:layout_width="280dip"
       android:layout_height="1dip"
       android:src="#454545" 
        
        />
     <LinearLayout 
      android:layout_width="300dip"
      android:layout_height="50dip" 
      android:orientation="horizontal"
      android:layout_marginTop="5dip"  
     >
     <TextView 
       android:layout_width="140dip"
       android:layout_height="40dip"
       android:text="6月-1年(含1年)"
       android:gravity="center"
       />     
      <TextView 
       android:layout_width="140dip"
       android:layout_height="40dip"
       android:text="6.56%"
       android:gravity="center"
       />   
    </LinearLayout>
     <ImageView
       android:layout_marginLeft="10dip" 
       android:layout_width="280dip"
       android:layout_height="1dip"
       android:src="#454545" 
        
        />
     <LinearLayout 
      android:layout_width="300dip"
      android:layout_height="50dip" 
      android:orientation="horizontal"
      android:layout_marginTop="5dip"  
     >
     <TextView 
       android:layout_width="140dip"
       android:layout_height="40dip"
       android:text="1-3年(含3年)"
       android:gravity="center"
       />     
      <TextView 
       android:layout_width="140dip"
       android:layout_height="40dip"
       android:text="6.65%"
       android:gravity="center"
       />   
    </LinearLayout>  
       <ImageView
       android:layout_marginLeft="10dip" 
       android:layout_width="280dip"
       android:layout_height="1dip"
       android:src="#454545" 
        
        />
     <LinearLayout 
      android:layout_width="300dip"
      android:layout_height="50dip" 
      android:orientation="horizontal"
      android:layout_marginTop="5dip"  
     >
     <TextView 
       android:layout_width="140dip"
       android:layout_height="40dip"
       android:text="3-5年(含5年)"
       android:gravity="center"
       />     
      <TextView 
       android:layout_width="140dip"
       android:layout_height="40dip"
       android:text="6.90%"
       android:gravity="center"
       />   
    </LinearLayout> 
       <ImageView
       android:layout_marginLeft="10dip" 
       android:layout_width="280dip"
       android:layout_height="1dip"
       android:src="#454545" 
        
        />
     <LinearLayout 
      android:layout_width="300dip"
      android:layout_height="50dip" 
      android:orientation="horizontal"
      android:layout_marginTop="5dip"  
     >
     <TextView 
       android:layout_width="140dip"
       android:layout_height="40dip"
       android:text="5-30年(含30年)"
       android:gravity="center"
       />     
      <TextView 
       android:layout_width="140dip"
       android:layout_height="40dip"
       android:text="7.05%"
       android:gravity="center"
       />   
    </LinearLayout> 
    </LinearLayout>.
  
</LinearLayout>
